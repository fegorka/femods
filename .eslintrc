{
	"root": true,
	"env": {
		"browser": true,
		"es2023": true,
		"node": true
	},
	"extends": [
		"plugin:vue/vue3-recommended",
		"eslint:recommended",
		"@vue/typescript/recommended"
	],
	"parserOptions": {
		"ecmaVersion": 2023
	},
	"plugins": [
	],
	"rules": {
		"vue/html-indent": [
			"error",
			"tab"
		],
		"indent": [
			"error",
			"tab"
		]
	}
}